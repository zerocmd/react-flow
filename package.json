{
  "name": "@frattaro/react-flow-renderer",
  "version": "10.0.7",
  "engines": {
    "node": ">=12"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wbkd/react-flow.git"
  },
  "scripts": {
    "build:lib": "babel src --out-dir lib --copy-files --extensions \".ts,.tsx\" ",
    "publish-package": "rimraf lib && npm ci && npm run build && npm publish",
    "build": "rollup -c --environment NODE_ENV:production && postcss src/*.css --dir dist",
    "start": "rollup -w -c",
    "start:examples": "cd example && npm start",
    "build:examples": "cd example && npm run build",
    "start:testserver": "npm run build && npm run start:examples",
    "build:dev": "npm install && npm run build && cd example && npm install && npm run build",
    "dev:wait": "start-server-and-test start:testserver http-get://localhost:3000",
    "test": "BROWSER=none npm run dev:wait test:chrome",
    "test:chrome": "cypress run --browser chrome --headless",
    "test:firefox": "cypress run --browser firefox",
    "test:all": "npm run test:chrome && npm run test:firefox",
    "cypress": "npm run dev:wait cy:open",
    "cy:open": "cypress open",
    "release": "npm run test && release-it",
    "release:notest": "release-it",
    "release:next": "release-it --preRelease=next --no-git"
  },
  "dependencies": {
    "@babel/runtime": "7.17.8",
    "classcat": "5.0.3",
    "d3-selection": "3.0.0",
    "d3-zoom": "3.0.0",
    "react-draggable": "4.4.4",
    "zustand": "3.7.1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.8",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@emotion/react": "11.8.2",
    "@types/d3": "7.1.0",
    "@types/react": "17.0.43",
    "@types/resize-observer-browser": "0.1.7",
    "babel-preset-react-app": "10.0.1",
    "cypress": "9.5.3",
    "prettier": "2.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "start-server-and-test": "1.14.0",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.8.2",
    "react": "16 || 17",
    "react-dom": "16 || 17"
  },
  "files": [
    "lib"
  ]
}